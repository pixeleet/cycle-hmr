{
  "name": "cycle-hmr",
  "version": "0.0.1",
  "description": "Cycle.js utilities for hot module replacement (HMR).",
  "main": "lib/index.js",
  "keywords": [
    "dx",
    "cyclejs",
    "hmr",
    "hot reload"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib",
    "build:watch": "babel src -w -d lib",
    "_test": "mocha --compilers js:babel-register test/index.js",
    "_test:watch": "npm run test -- --watch",
    "test:watch": "node-dev --respawn -r babel-register test",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "whitecolor",
  "license": "ISC",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-cycle-circular": "^0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "mocha": "^2.4.5",
    "rx": "^4.1.0",
    "tape": "^4.5.1"
  },
  "dependencies": {}
}
